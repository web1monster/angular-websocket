  "dependencies": {
    "angular": "*",
    "ws": "^1.0.0"
  },
  "devDependencies": {
    "angular-animate": "^1.3.13",
    "babel-cli": "^6.7.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "browserify": "^13.0.0",
    "browserify-ngannotate": "^2.0.0",
    "bufferutil": "^1.2.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.22",
