    "karma-slimerjs-launcher": "^0.2.0",
    "mkdirp": "^0.5.1",
    "ng-annotate": "^1.0.0",
    "phantomjs": "^2.1.3",
    "phantomjs-prebuilt": "^2.1.7",
    "requirejs": "^2.1.16",
    "rimraf": "^2.5.2",
    "uglify-js": "^2.4.16",
    "utf-8-validate": "^1.2.1"
